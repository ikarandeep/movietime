  <body>
<section id="home">
<img src="../img/movietimelogo.png">

  <div class="jumbotron">
    <div class="container">
<form class="form-horizontal" role="form">
  <div class="form-group">
    <label for="inputEmail1" class="col-lg-2 control-label">Email</label>
    <div class="col-lg-10">
      <input style="width:500px" name="email" type="text" class="form-control" id="inputEmail" placeholder="Email">
    </div>
  </div>
  <div class="form-group">
      <label for="inputPhone" class="col-lg-2 control-label">Mobile Number</label>
    <div class="col-lg-10">
      <input style="width:500px" name="phone" type="number" class="form-control" id="inputPhone" placeholder="Phone Number">
    </div>
  </div>
      <label for="inputInterests" class="col-lg-2 control-label">Interests</label>
        <select style="width:500px" multiple id="e2_2" name="interests" class="form-control">
           <option value="Action &amp; Adventure">Action &amp; Adventure</option>
                    <option value="Animation" >Animation</option>
                    <option value="Anime &amp; Manga" >Anime &amp; Manga</option>
                    <option value="Art House &amp; International" >Art House &amp; International</option>
                    <option value="Classics" >Classics</option>
                    <option value="Comedy" >Comedy</option>
                    <option value="Cult Movies" >Cult Movies</option>
                    <option value="Documentary" >Documentary</option>
                    <option value="Drama" >Drama</option>
                    <option value="Faith &amp; Spirituality" >Faith &amp; Spirituality</option>
                    <option value="Gay &amp; Lesbian" >Gay &amp; Lesbian</option>
                    <option value="Horror" >Horror</option>
                    <option value="Kids &amp; Family" >Kids &amp; Family</option>
                    <option value="Musical &amp; Performing Arts">Musical &amp; Performing Arts</option>
                    <option value="Mystery &amp; Suspense" >Mystery &amp; Suspense</option>
                    <option value="Romanace" >Romance</option>
                    <option value="Science Fiction &amp; Fantasy" >Science Fiction &amp; Fantasy</option>
                    <option value="Special Interests" >Special Interest</option>
                    <option value="Sports &amp; Fitness" >Sports &amp; Fitness</option>
                    <option value="Television" >Television</option>
                    <option value="Western" >Western</option>
        </select>
    <div class="form-group">
      <div class="col-lg-10 col-lg-offset-2">
        <button class="btn btn-default" id='submitUserInfo'>Submit</button>
      </div>
    </div>
</form>
<span id="insertHere"></span>
</section>


