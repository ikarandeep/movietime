    <div class="container">



      <!-- Forms
      ================================================== -->
      <div class="bs-docs-section">
        <div class="row">
          <h1 style="text-align: center"> Register </h1>
          </div>
        </div>

        <div class="row">
          <div class="col-lg-6" style="width: inherit; margin-left: 20%">
            <div class="well">
              <form class="bs-example form-horizontal">
                <fieldset>
                  <legend>Legend</legend>
                  <div class="form-group">
                    <label for="inputEmail" class="col-lg-2 control-label">Email</label>
                    <div class="col-lg-10">
                      <input name="email" type="text" class="form-control" id="inputEmail" placeholder="Email">
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="inputPhone" class="col-lg-2 control-label">Mobile Number</label>
                    <div class="col-lg-10">
                        <input name="phone" type="number" class="form-control" id="inputPhone" placeholder="Phone Number">
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="inputInterests" class="col-lg-2 control-label">Interests</label>
                    <div class="col-lg-10">
                      <input type="text" name="interests" class="form-control" id="inputInterests" placeholder="Interests (ex: Fiction, Action, Romance)">
                  </div>
                  <div class="form-group">
                    <div class="col-lg-10 col-lg-offset-2">
                      <button class="btn btn-default" id='submitUserInfo'>Submit</button>
                    </div>
                  </div>
                </fieldset>
              </form>
            </div>
          </div>

        </div>
      </div>


